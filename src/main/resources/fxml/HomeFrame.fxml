    <VBox fx:id="oldFrameBox"/>
    <VBox fx:id="home" prefHeight="500.0" prefWidth="800.0" stylesheets="@css/HomeFrame.css">
        <VBox fx:id="main" onMouseClicked="#closeMenu">

        </VBox>
        <VBox fx:id="menu" styleClass="menu" translateX="-140" translateY="-500">
            <Text fx:id="name" styleClass="name" text="啊啊啊啊啊啊啊啊"/>
            <Text fx:id="id" styleClass="id" text="111111111111"/>
        </VBox>
        <ImageView fx:id="head" fitHeight="50.0" fitWidth="50.0" preserveRatio="true" translateX="5" translateY="-995" onMouseClicked="#headAction"/>
    </VBox>
</AnchorPane>
